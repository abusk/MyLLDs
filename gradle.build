plugins {
    id 'java'
    id 'application'
}
allprojects {
    apply plugin: 'java-library'
    java {
        toolchain {
            languageVersion = JavaLanguageVersion.of(21)
        }
    }
    group = 'org.myllds'
    version = '1.0-SNAPSHOT'

    repositories {
        mavenCentral()
    }
}